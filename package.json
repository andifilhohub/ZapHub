{
  "name": "zaphub",
  "version": "1.0.0",
  "main": "src/server/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/server/app.js",
    "dev": "nodemon src/server/app.js",
    "worker": "node src/workers/index.js",
    "worker:events": "node src/workers/event-workers.js",
    "db:create": "node src/db/createDb.js",
    "db:migrate": "node src/db/migrate.js",
    "db:seed": "node src/db/seed.js",
    "test:db:create": "node src/db/createTestDb.js",
    "test:db:migrate": "node src/db/migrateTest.js",
    "test:db:reset": "node src/db/resetTestDb.js && npm run test:db:migrate",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "npm run test -- tests/unit",
    "test:integration": "npm run test -- tests/integration",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:setup": "npm run test:db:create",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "whatsapp",
    "baileys",
    "api",
    "webhook",
    "queue"
  ],
  "author": "",
  "license": "ISC",
  "description": "Plataforma de múltiplas conexões WhatsApp usando Baileys",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^7.0.0-rc.8",
    "axios": "^1.13.2",
    "bullmq": "^5.34.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "npm": "^11.6.4",
    "pg": "^8.13.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "supertest": "^7.1.4"
  }
}
